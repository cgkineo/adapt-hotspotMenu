<div class="menu-container">
	<div class='menu-container-inner box-menu-inner clearfix'>
		<div class="menu-container-intro">
			{{#if title}}
			<div class="menu-title" role="heading">
				<h1 class="menu-title-inner">
					{{{title}}}
				</h1>
			</div>
			{{/if}}
			{{#if body}}
			<div class="menu-body">
				<div class="menu-body-inner">
					{{{body}}}
				</div>
			</div>
			{{/if}}
		</div>
		<div class="hotspot-container">
			<div class="hotspot-container-inner">
				<img class="hotspot-container-image" src="{{_hotspotMenuGraphic.src}}" alt="{{_hotspotMenuGraphic.alt}}" />
				<div class="hotspot-items clearfix">

				</div>
			</div>
		</div>
	</div>
</div>